<!DOCTYPE databases PUBLIC
"-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
"http://castor.exolab.org/mapping.dtd">
<mapping>

<description>A mapping settings for Behavior class</description>

<class name="com.klatencor.klara.future.object.recipe.Behavior" auto-complete="true">
    <map-to xml="Behavior"/>
    <field name="autoSetup">
        <bind-xml name="AutoSetup" node="attribute"/>
    </field>
    <field name="autoInspect">
        <bind-xml name="AutoInspect" node="attribute"/>
    </field>
    <field name="autoReview">
        <bind-xml name="AutoReview" node="attribute"/>
    </field>
    <field name="autoUnload">
        <bind-xml name="AutoUnload" node="attribute"/>
    </field>
    <field name="abortJobOnError">
        <bind-xml name="AbortJobOnError" node="attribute"/>
    </field>
    <field name="printAfterInspection">
        <bind-xml name="PrintAfterInspection" node="attribute"/>
    </field>
    <field name="printAfterReview">
        <bind-xml name="PrintAfterReview" node="attribute"/>
    </field>
    <field name="printFullReport">
        <bind-xml name="PrintFullReport" node="attribute"/>
    </field>
    <field name="storeAfterInspection">
        <bind-xml name="StoreAfterInspection" node="attribute"/>
    </field>
    <field name="storeImages">
        <bind-xml name="StoreImages" node="attribute"/>
    </field>
    <field name="storeAfterReview">
        <bind-xml name="StoreAfterReview" node="attribute"/>
    </field>
    <field name="displayNewDefects">
        <bind-xml name="DisplayNewDefects" node="attribute"/>
    </field>
    <field name="iadDefectConcat">
        <bind-xml name="IadDefectConcat" node="attribute"/>
    </field>
    <field name="vDLEnabled">
        <bind-xml name="VDLEnabled" node="attribute"/>
    </field>
    <field name="vDLArrayX">
        <bind-xml name="VDLArrayX" node="attribute"/>
    </field>
    <field name="vDLFilename">
        <bind-xml name="VDLFilename" node="attribute"/>
    </field>
    <field name="inspectionCount">
        <bind-xml name="InspectionCount" node="attribute"/>
    </field>
    <field name="abortFailCount">
        <bind-xml name="AbortFailCount" node="attribute"/>
    </field>
    <field name="abortWarnCount">
        <bind-xml name="AbortWarnCount" node="attribute"/>
    </field>
    <field name="zCalForP90">
        <bind-xml name="ZCalForP90" node="attribute"/>
    </field>
    <field name="zCalForP125">
        <bind-xml name="ZCalForP125" node="attribute"/>
    </field>
    <field name="autoDnir">
        <bind-xml name="AutoDnir" node="attribute"/>
    </field>
    <field name="diagCalType">
        <bind-xml name="DiagCalType" node="attribute"/>
    </field>
    <field name="vdlParameters">
        <bind-xml name="VdlParameters" node="attribute"/>
    </field>
    <field name="spotCal">
        <bind-xml name="SpotCal" node="attribute"/>
    </field>
        
</class>

</mapping>