<!DOCTYPE databases PUBLIC
"-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
"http://castor.exolab.org/mapping.dtd">
<mapping>

<description>A mapping settings for PlateAlignment class</description>

<class name="com.klatencor.klara.future.object.recipe.PlateAlignment" auto-complete="true">
    <map-to xml="PlateAlignment"/>
    <field name="inspectionMethod">
        <bind-xml name="InspectionMethod" node="attribute"/>
    </field>
    <field name="acquisitionView">
        <bind-xml name="AcquisitionView" node="attribute"/>
    </field>
    
    <field name="alignmentPointInfo" type="com.klatencor.klara.future.object.recipe.PlateAlignment$AlignmentPointInfo">
        <bind-xml name="AlignmentPointInfo"/>
    </field>
    <field name="alignmentPoints" collection="arraylist" type="com.klatencor.klara.future.object.recipe.PlateAlignment$AlignmentPoint" >
        <bind-xml name="AlignmentPoint" location="AlignmentPoints"/>
    </field>
    <field name="databaseAlignmentPointInfo" type="com.klatencor.klara.future.object.recipe.PlateAlignment$DatabaseAlignmentPointInfo">
        <bind-xml name="DatabaseAlignmentPointInfo"/>
    </field>
    <field name="databaseAlignmentPoints" collection="arraylist" type="com.klatencor.klara.future.object.recipe.PlateAlignment$DatabaseAlignmentPoint" >
        <bind-xml name="DatabaseAlignmentPoint" location="DatabaseAlignmentPoints"/>
    </field>
</class>

<class name="com.klatencor.klara.future.object.recipe.PlateAlignment$AlignmentPointInfo" auto-complete="true">
    <field name="lightMode">
        <bind-xml name="LightMode" node="attribute"/>
    </field>
    <field name="magnification">
        <bind-xml name="Magnification" node="attribute"/>
    </field>
    <field name="alignmentType">
        <bind-xml name="AlignmentType" node="attribute"/>
    </field>
</class>

<class name="com.klatencor.klara.future.object.recipe.PlateAlignment$AlignmentPoint" auto-complete="true">
    <field name="type">
        <bind-xml name="Type" node="attribute"/>
    </field>
    <field name="x">
        <bind-xml name="X" node="attribute"/>
    </field>
    <field name="y">
        <bind-xml name="Y" node="attribute"/>
    </field>
</class>

<class name="com.klatencor.klara.future.object.recipe.PlateAlignment$DatabaseAlignmentPointInfo" auto-complete="true">
    <field name="lightMode">
        <bind-xml name="LightMode" node="attribute" />
    </field>
     <field name="magnification">
        <bind-xml name="Magnification" node="attribute" />
    </field>
     <field name="alignmentType">
        <bind-xml name="AlignmentType" node="attribute" />
    </field>
    
</class>

<class name="com.klatencor.klara.future.object.recipe.PlateAlignment$DatabaseAlignmentPoint" auto-complete="true">
    <field name="type">
        <bind-xml name="Type" node="attribute"/>
    </field>
    <field name="x">
        <bind-xml name="X" node="attribute"/>
    </field>
    <field name="y">
        <bind-xml name="Y" node="attribute"/>
    </field>
</class>

</mapping>